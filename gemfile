source 'https://rubygems.org'

gem "jekyll"
gem "glynn"
gem "rake"
#gem "mini_magick"

#gem "wombat"
gem "pry"
gem "rubyzip"
gem 'jekyll-sitemap'

group :jekyll_plugins do
	gem "jekyll-coffeescript" # We want to write our javascripts in CoffeeScript
	gem "uglifier"      # And we want our javascripts to be minified with UglifyJS
	gem "sass"          # And we want to write our stylesheets using SCSS/SASS
    gem "jekyll-assets"
end
